{
  "version": 2,
  "builds": [
    {
      "src": "api/src/index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": "api/**"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/src/index.js"
    }
  ],
  "functions": {
    "api/src/index.js": {
      "maxDuration": 60
    }
  }
}
